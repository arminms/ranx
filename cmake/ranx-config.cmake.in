@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

list(APPEND CMAKE_MODULE_PATH ${CMAKE_CURRENT_LIST_DIR}/modules)

include ( "${CMAKE_CURRENT_LIST_DIR}/ranx-targets.cmake" )

foreach(_comp ${ranx_FIND_COMPONENTS})
  if (${_comp} STREQUAL cuda)
    find_package(CUDAToolkit 11 QUIET)
  elseif(${_comp} STREQUAL oneapi)
    find_package(IntelDPCPP QUIET)
    find_package(oneDPL QUIET)
  elseif(${_comp} STREQUAL rocm)
    find_package(hip QUIET)
    find_package(rocThrust QUIET)
  elseif(${_comp} STREQUAL openmp)
    find_dependency(OpenMP)
  else()
    set(ranx_FOUND False)
    set(ranx_NOT_FOUND_MESSAGE "Unsupported ranx target API: ${_comp}")
  endif()
  include("${CMAKE_CURRENT_LIST_DIR}/ranx-${_comp}-targets.cmake")
endforeach()
